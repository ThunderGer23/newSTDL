                                        <th scope="col">Asunto</th>
                                        <th scope="col">Derivado a</th>
                                        <th scope="col">Ver</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><input type="checkbox"></td>
                                        <td>sin</td>
                                        <td>resultados</td>
