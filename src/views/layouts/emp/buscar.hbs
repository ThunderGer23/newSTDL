<!-- Begin page -->
<div id="layout-wrapper">
    {{>headeremp}}
    {{>sidebaremp}}
    <div class="main-content">
        <div class="page-content">
            <div class="container-fluid">

                <!-- start page title -->
                        {{>pagetitle}}
                        <!-- end page title -->
                <!-- Start row -->
                 <form action="/search">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Registro</h4>
                                <p class="card-title-desc">Seleccione la fecha.</p>
                            </div>
                            <div class="card-body">
                               
                                    <div class="mb-4">
                                        <label class="form-label" for="fecha-desde-">Fecha desde</label>
                                        <input class="form-control" type="date" id="fecha-desde">
                                    </div>
                                    <div class="mb-4">
                                        <label class="form-label" for="fecha-hasta">Fecha hasta</label>
                                        <input class="form-control" type="date" id="fecha-hasta">
                                    </div>
                                
                            </div>
                        </div>
                    </div>
                    <!-- end col -->
                    <div class="col-lg-6">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Orígen del expediente
                                </h4>
                                <p class="card-title-desc">Seleccione si es externo o interno</p>
                            </div>
                            <div class="card-body">
                                <div class="mb-3">
                                    <label for="">Orígen</label> &nbsp;&nbsp;
                                    <span class="me-3">Interno</span><input type="radio">&nbsp;
                                    <span class="me-3">Extermo</span><input type="radio">
                                </div>

                                <div class="mb-4">
                                    <label for="">Dependencia</label>
                                    <select name="" id="" class="form-control">
                                        <option value="1">-----Todos------</option>
                                        <option value="2">Gerencia</option>
                                        <option value="3">Tesorería</option>
                                        <option value="4">Secretaría</option>
                                    </select>
                                </div>

                                <div>
                                    <label for="">Detalle</label>
                                    <input name="detalle" type="text" class="form-control">
                                </div>
                                <div>
                                    <label for="">Firma</label>
                                    <input name="firma" type="text" class="form-control">
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- end col -->
                </div>
                <!-- End row -->
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Datos del expediente</h4>
                                <p class="card-title-desc"></p>
                            </div>
                            <div class="card-body p-4">

                                <div class="row">
                                    <div class="col-lg-6">
                                        <div>
                                            <div class="mb-3">
                                                <label for="">Tipo de expediente</label>
                                                <select name="" id="" class="form-control">
                                                    <option value="1">-----Todos------</option>
                                                    <option value="2">Oficio</option>
                                                    <option value="3">Carta</option>
                                                    <option value="4">Memorandum</option>
                                                </select>
                                            </div>
                                            <div class="mb-3">
                                                <label for="">Siglas</label>
                                                <input type="text" class="form-control">
                                            </div>

                                        </div>
                                    </div>

                                    <div class="col-lg-6">
                                        <div class="mt-3 mt-lg-0">
                                            <div class="mb-3">
                                                <label for="">Número</label>
                                                <input type="number" class="form-control">
                                            </div>
                                            <div class="mb-3">
                                                <label for="">Asunto</label>
                                                <input type="text" class="form-control">
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div> <!-- end col -->
                </div>
                <div class="col-12">
                    <button class="btn btn-primary">Buscar</button>
                </div>
                </form>
                <!-- end row -->
                {{!-- end row --}}
            

            </div><!-- end col -->
        </div>
        <!-- end row-->
        {{!--resposive table --}}
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">Resultado de la busqueda</h4>
                        <p class="card-title-desc">

                        </p>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table mb-0">
                                <thead>
                                    <tr>
                                        <th scope="col"><input type="checkbox"></th>
                                        <th scope="col">Reg</th>
                                        <th scope="col">Origen</th>
                                        <th scope="col">Firma</th>
