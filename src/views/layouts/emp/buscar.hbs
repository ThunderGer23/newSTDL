                                        <td>mostrar</td>
                                        <td>intente</td>
                                        <td>con</td>
                                        <td>otra</td>
                                        <td>busqueda</td>
                                        <td>.</td>
                                    </tr>

                                </tbody>
                                <tfoot>
                                    <tr>
                                        <th scope="col"><input type="checkbox"></th>
                                        <th scope="col">Reg</th>
                                        <th scope="col">Origen</th>
                                        <th scope="col">Firma</th>
                                        <th scope="col">Cargo</th>
                                        <th scope="col">Documento</th>
                                        <th scope="col">Fecha Doc.</th>
                                        <th scope="col">Asunto</th>
                                        <th scope="col">Derivado a</th>
                                        <th scope="col">Ver</th>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>

                    </div>
                    <!-- end card body -->
                </div>
                <!-- end card -->
            </div>
            <!-- end col -->
        </div>
        <!-- end row -->
    </div>
    <!-- container-fluid -->
</div>
<!-- End Page-content -->



{{>footer}}
